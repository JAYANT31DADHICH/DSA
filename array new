#include<iostream>
using namespace std;

int Newarr(int ar[],int length){
	

}

int SumArray(int array[],int size){
	int sum=0;
	for(int i=0;i<=size;i++){
	     sum=sum+array[i];
	}
	return sum;
}


int main(){
	
	
	cout<<"enter the size of array"<<" ";
	int length;
	cin>>length;
	int ar[100]={};
	for(int i=0;i<=length;i++){
		cin>>ar[i];
	}
	cout<<SumArray(ar,length);
	return 0;
}
