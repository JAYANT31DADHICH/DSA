#include<iostream>
using namespace std;

void Newarr(int ar[],int n){
	for(int i=0;i<n;i++){
	ar[i]=ar[i]+1;
	}

}




int main(){
	
	
	cout<<"enter the size of array"<<" ";
	int length;
	cin>>length;
	int ar[100]={};
	for(int i=0;i<length;i++){
		cin>>ar[i];
	}
	Newarr(ar,length);
	for(int i=0;i<length;i++){
		cout<<ar[i]<<" ";
	}
	
	return 0;
}



#include<iostream>
using namespace std;

void Newarr(int ar[],int n){
	int start=0;
	for(int i=0;i<n;i++){
		start=ar[i];
		start=start+1;
	//ar[i]=ar[i]+1;
	ar[i]=start;
	}

}




int main(){
	
	
	cout<<"enter the size of array"<<" ";
	int length;
	cin>>length;
	int ar[100]={};
	for(int i=0;i<length;i++){
		cin>>ar[i];
	}
	Newarr(ar,length);
	for(int i=0;i<length;i++){
		cout<<ar[i]<<" ";
	}
	
	return 0;
}
